[
    {
      "name": "constellationjs-container",
      "image": "464116869262.dkr.ecr.us-east-1.amazonaws.com/constellationjs:latest",
      "cpu": 10,
      "memory": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8082
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": { 
          "awslogs-group" : "/ecs/constellationjs-container",
          "awslogs-region": "us-east-1"
        }
      }
    }
]